extends ../layout
block content
  //<!-- !PAGE CONTENT! -->
  .w3-main(style="margin-left:340px;margin-right:40px")

    //<!-- Modal for full size images on click-->
    .w3-modal.w3-black.w3-padding-0(id="modal" onclick="this.style.display='none'")
      span.w3-closebtn.w3-text-white.w3-opacity.w3-hover-opacity-off.w3-xxlarge.w3-container.w3-display-topright &times;</span>
      div.w3-modal-content.w3-animate-zoom.w3-center.w3-transparent.w3-padding-64
        img.w3-image(id="img")
        p(id="caption")
